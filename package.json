{
  "name": "y-nostr",
  "version": "0.1.0",
  "description": "Nostr provider for yjs",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/y-nostr.d.ts",
        "default": "./lib/esm/y-nostr.js"
      },
      "require": {
        "types": "./lib/cjs/types/y-nostr.d.ts",
        "default": "./lib/cjs/y-nostr.js"
      }
    }
  },
  "types": "./lib/cjs/types/y-nostr.d.ts",
  "main": "./lib/cjs/y-nostr.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest",
    "clean": "npm run clean:win && npm run clean:lin",
    "clean:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist lib rmdir /Q /S lib",
    "clean:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf lib",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pirxthedev/y-nostr.git"
  },
  "keywords": [],
  "author": "Pirx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pirxthedev/y-nostr/issues"
  },
  "homepage": "https://github.com/pirxthedev/y-nostr#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "jest-websocket-mock": "^2.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "websocket-polyfill": "^0.0.3"
  },
  "dependencies": {
    "js-base64": "^3.7.5",
    "lib0": "^0.2.63",
    "nostr-tools": "^1.7.1",
    "yjs": "^13.5.47"
  }
}
